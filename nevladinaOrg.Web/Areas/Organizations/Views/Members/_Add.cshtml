    // WIZARD CONSTRUCTOR
    var WizardDemo = function () {
        $("#m_wizard");
        var e,
            r,
            i = $("myForm");
        return {
            init: function () {
                var n;
                $("#m_wizard"),
                    i = $("#myForm"),
                    (r = new mWizard("m_wizard", {
                    //ON LOAD SET GO TO CONFIRMATION OR ON FIRST STEP
                    @if (Model.CurrentStep != 4)
                    {
                        @:startStep: 1
                    }
                    else
                    {
                        @:startStep: 4
                    }
                    }
                    )).on("beforeNext", function (r) {
                    !0 !== e.form() && r.stop();
                    //RANGE VALIDATION SET AFTER INPUT
                    SetPositionSelectpickerValidation();

                    $("#genderM").siblings(".form-control-feedback").insertAfter($("#maleGenderDiv"));

                    //IF 1 STEP GENERATE USERNAME FROM NAME AND SURNAME
                    if (r.currentStep == 1) {
                        $("#Person_CountryId").valid()
                        var firstname = $("#Person_FirstName").val();
                        var lastname = $("#Person_LastName").val();
                        if (firstname == "" || lastname == "") {
                            e.form();
                            r.stop();
                        }
                        else {
                            $.ajax({
                                dataType: "JSON",
                                method: "GET",
                                url: "@Url.Action("GenerateUsername", "Members", new { area = "Organizations" })",
                                data: { firstname: firstname, lastname: lastname },
                                cache: false,
                                success: function (data) {
                                    $("#User_Username").val(data.username);
                                }
                            });
                        }
                    }
                    
                    //ON 3. STEP - VALIDATE AND STOP WIZARD, IF VALID THEN SUBMIT
                    else if (r.currentStep == 3) {
                        if (e.form()){
                            r.stop(),
                             $("#myForm")[0].submit()
                        }
                    }

                    }
                    ),
                    r.on("change", function (e) {
                    $("#CurrentStep").val(r.currentStep),
                        mUtil.scrollTop()
                    }),
                    
                    e = i.validate({
                        ignore: ":hidden", rules: {
                            "Person.FirstName": {
                                required: 1
                            }
                            , "Person.LastName": {
                                required: 1
                            }
                            , "Person.ParentName": {
                                required: 1
                            }
                            , "Person.DateOfBirth": {
                                required: 1
                            }
                            , "Person.CountryId": {
                                required: 1
                            }
                            , "Person.CityId": {
                                range: [1, 100000]
                            }
                            , "Person.Gender": {
                                required: 1
                            }
                            , "PersonPhone.Contact": {
                                required: 1,
                                regex: "^0[23456789]{1}[0-9]{7,8}$"
                            }
                            , "PersonPrimaryEmail.Contact": {
                                required: 1,
                                email: !0
                            }
                            , "PersonDetail.AcademicDegreeId": {
                                range: [1, 100000],
                                required:1
                            }
                            , "License.LicenceNumber": {
                                required: $("#Member:checked").val() == true ? 1 : 0
                            }
                            , "LicensePeriod.CreatedDate":{
                                required: $(".licenceType:checked").val() == "perPeriod" ? 1 : 0
                            }
                            , "LicensePeriod.EndDate":{
                                required: $(".licenceType:checked").val() == "perPeriod" ? 1 : 0
                            }
                            , "User.Password": {
                                required: 1,
                                regex: "(?=.*[0-9])(?=.*[a-zA-Z]).",
                                minlength:6
                            }
                            , "User.Username": {
                                required:1
                            }
                            , "User.ConfirmPassword": {
                                equalTo: "#User_Password"
                            }
                            , "User.Email": {
                                required: 1,
                                email: 1
                            }
                            , "Roles": {
                                required: 1
                            }

                        }
                        , messages: {
                            "account_communication[]": {
                                required: "You must select at least one communication option"
                            }
                            , accept: {
                                required: "You must accept the Terms and Conditions agreement!"
                            }
                            , "Person.FirstName": {
                                required: "@Localizer.ErrorMessageFirstNameReq"
                            }
                            , "Person.LastName": {
